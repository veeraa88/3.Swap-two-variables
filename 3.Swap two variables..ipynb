   ],
   "source": [
    "a=input(\"Enter the first string:\")\n",
    "b=input(\"Enter the second string:\")\n",
    "print(f\"original value: {a} and {b}\")\n",
    "temp=a\n",
    "a=b\n",
    "b=temp\n",
    "print(a)\n",
    "#print(f\"swapped value: a = {a} , b = {b}\")"
